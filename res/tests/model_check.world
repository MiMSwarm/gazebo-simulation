<?xml version="1.0"?>

<!-- To test a single robot -->
<sdf version="1.6">
  <world name="model_setup">

    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Cool robots -->
    <include>
      <uri>model://minimapper</uri>
    </include>

  </world>
</sdf>