<?xml version="1.0"?>

<!-- To test a single robot -->
<sdf version="1.6">
  <world name="sonar_check">

    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Cool robots -->
    <include>
      <uri>model://minimapper</uri>
    </include>

    <!-- The Bobs -->
    <population name="stormbobs">
      <model_count>15</model_count>
      <distribution>
        <type>random</type>
      </distribution>
      <box>
        <size>5 10 0.15</size>
      </box>
      <pose>2.65 0 0 0 0 0</pose>
      <model name="bob">
        <include>
          <uri>model://bob</uri>
        </include>
      </model>
    </population>

  </world>
</sdf>