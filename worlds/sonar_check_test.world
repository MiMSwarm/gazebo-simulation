<?xml version="1.0"?>

<!-- To test a single robot -->
<sdf version="1.6">
  <world name="sonar_check">

    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Cool robots -->
    <include>
      <uri>model://sonar</uri>
    </include>

    <include>
      <name>bob1</name>
      <uri>model://bob</uri>
      <pose>1 1 0 0 0 0</pose>
    </include>

    <include>
      <name>bob2</name>
      <uri>model://bob</uri>
      <pose>2 2 0 0 0 0</pose>
    </include>

    <include>
      <name>bob3</name>
      <uri>model://bob</uri>
      <pose>2 0 0 0 0 0</pose>
    </include>

    <include>
      <name>bob4</name>
      <uri>model://bob</uri>
      <pose>1 -1 0 0 0 0</pose>
    </include>

    <include>
      <name>bob5</name>
      <uri>model://bob</uri>
      <pose>2 -2 0 0 0 0</pose>
    </include>

  </world>
</sdf>