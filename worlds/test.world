<?xml version="1.0"?>

<!-- To test a single robot -->
<sdf version="1.6">
  <world name="test_world">

    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Cool robots -->
    <population name="swarm">
      <pose>0 0 5 0 0 0</pose>
      <box><size>10 10 10</size></box>
      <model_count>5</model_count>
      <model name="minimapper">
        <include><uri>model://minimapper</uri></include>
      </model>
    </population>

  </world>
</sdf>